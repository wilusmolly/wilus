	<script type="text/javascript">
		$(document).ready(function(){
			//도움말
			$('#timeline-help').on('click',function(){
				$('.wrap-pihelpconts').show();
			});

			//도움말 닫기
			$('#timeline-closeHelp').on('click',function(){
				$('.wrap-pihelpconts').hide();
			});

			//홈으로가기
			$('#timeline-home').on('click',function(){
				location.href = "/";
			});

			$(window).unbind('scroll');
		});
	</script>
